{
  "name": "Product Reviews",
  "description": "Product reviews section with star ratings and customer feedback",
  "settings": {
    "title": {
      "type": "string",
      "label": "Section Title",
      "default": "التقييم و المراجعات"
    },
    "rating": {
      "type": "number",
      "label": "Average Rating",
      "default": 4.5
    },
    "totalReviews": {
      "type": "number",
      "label": "Total Reviews",
      "default": 30
    }
  },
  "blocks": {
    "reviewItem": {
      "name": "Review Item",
      "settings": {
        "name": {
          "type": "text",
          "label": "Customer Name",
          "default": "احمد محمد"
        },
        "stars": {
          "type": "number",
          "label": "Stars",
          "default": 5,
          "min": 1,
          "max": 5
        },
        "review": {
          "type": "textarea",
          "label": "Review Text",
          "default": "أنا سعيدة جدًا بقمصاني! طلبت مقاس 8XL على أمل أن تناسبني، وهي واسعة جدًا! تصاميمها رائعة! جودة ممتازة، قطن سميك. أنصح بها بشدة."
        }
      }
    }
  },
  "presets": [
    {
      "category": "الصفحة الرئيسية",
      "name": "⭐ Product Reviews",
      "settings": {
        "title": "التقييم و المراجعات",
        "rating": 4.5,
        "totalReviews": 30
      },
      "blocks": [
        {
          "block": "reviewItem",
          "name": "First Review",
          "settings": {
            "name": "احمد محمد",
            "stars": 5,
            "review": "أنا سعيدة جدًا بقمصاني! طلبت مقاس 8XL على أمل أن تناسبني، وهي واسعة جدًا! تصاميمها رائعة! جودة ممتازة، قطن سميك. أنصح بها بشدة."
          }
        }
      ]
    }
  ]
}